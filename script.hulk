type Point(x : Number,y : Number) {
    x = x;
    y = y;

    getX() => self.x;
    getY() => self.y;

    setX(x) => self.x := x;
    setY(y) => self.y := y;
}

type PolarPoint inherits Point {
    rho() => sqrt(self.getX() ^ 2 + self.getY() ^ 2);
}

let pt = new PolarPoint(3,4) in {
    pt.setX(5);
    pt.setY(12);
    print("X: " @ pt.getX());
    print("Y: " @ pt.getY());
    print("Rho: " @ pt.rho());
}